<?php

/**
 * Implements hook_library_info_build()
 *
 * Add Etherpad jQuery Plugin library to drupal library.
 */
function ce_etherpad_library_info_build() {
  $library['ce_etherpad.jquery_plugin'] = [
    'js' => [
      '/libraries/ce_etherpad/js/etherpad.js' => [],
    ],
  ];
  return $library;
}